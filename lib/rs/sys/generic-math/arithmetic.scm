(define-method binary+ ((a <fixnum>) (b <fixnum>))
  (fixnum+ a b))

(define-method binary+ ((a <double-float>) (b <double-float>))
  (float+ a b))

(define-method binary+ ((a <double-float>) (b <double-float>))
  (float+ a b))

(define-method binary+ ((a <double-float>) (b <double-float>))
  (float+ a b))

